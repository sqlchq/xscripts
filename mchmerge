/* Basic Script Extract Duplicates from PARENT and CHILD */
SELECT *
FROM 'parent obj`
WHERE  EMailAddress
IN
(SELECT EmailAddress
FROM 'child obj`)


/* View Creation - to create Duplicates list as an object */
CREATE OR UPDATE VIEW DupEmails AS
SELECT *
FROM 'parent obj`
WHERE  EMailAddress
IN
(SELECT EmailAddress
FROM 'child obj`)

